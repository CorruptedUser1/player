-- Scripts:

local function MISKEV_fake_script() -- Handler.Main 
	local script = Instance.new('LocalScript', Handler)

	wait(game:IsLoaded())
	---------------------------------------------------------------------------------------------------------------[ Stroke Giver ]
	--------------------------------------------------- mainFrame
	local strokeNew = Instance.new('UIStroke')
	strokeNew.Parent = script.Parent
	local clone1 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone1.Parent = script.Parent.Parent
	clone1.Thickness = 2
	clone1.ApplyStrokeMode = 'Border'
	clone1.Color = Color3.fromRGB(255, 162, 0)
	clone1.Enabled = true
	--------------------------------------------------- plrButtons
	local clone2 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone2.Thickness = 2
	clone2.ApplyStrokeMode = 'Border'
	clone2.Color = Color3.fromRGB(255, 162, 0)
	clone2.Enabled = true
	clone2.Parent = script.Parent.TextButton
	--------------------------------------------------- Button
	for i,q in pairs(script.Parent.Parent.infoFrame.holderBtn:GetChildren()) do
		if q:IsA('TextButton') then
			local clone3 = script.Parent:WaitForChild('UIStroke'):Clone()
			clone3.Thickness = 1
			clone3.ApplyStrokeMode = 'Border'
			clone3.Color = Color3.fromRGB(255, 162, 0)
			clone3.Enabled = true
			clone3.Parent = q
		end
	end
	--------------------------------------------------- Image
	local clone4 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone4.Thickness = 2
	clone4.ApplyStrokeMode = 'Border'
	clone4.Color = Color3.fromRGB(0, 0, 0)
	clone4.Enabled = true
	clone4.Parent = script.Parent.Parent.infoFrame.plrImage
	--------------------------------------------------- holder
	local clone5 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone5.Thickness = 2
	clone5.ApplyStrokeMode = 'Border'
	clone5.Color = Color3.fromRGB(0, 0, 0)
	clone5.Enabled = true
	clone5.Parent = script.Parent.Parent.infoFrame.Frame
	--------------------------------------------------- holder
	local clone6 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone6.Thickness = 2
	clone6.ApplyStrokeMode = 'Border'
	clone6.Color = Color3.fromRGB(0, 0, 0)
	clone6.Enabled = true
	clone6.Parent = script.Parent.Parent.plrHolder
	--------------------------------------------------- holder
	local clone7 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone7.Thickness = 2
	clone7.ApplyStrokeMode = 'Border'
	clone7.Color = Color3.fromRGB(0, 0, 0)
	clone7.Enabled = true
	clone7.Parent = script.Parent.Parent.infoFrame
	
	script.Parent.Parent.plrHolder.mainPlrHolder.ScrollBarImageTransparency = 1
	
	local womp = Instance.new('StringValue')
	womp.Name = 'plr'
	womp.Parent = script.Parent.Parent.infoFrame.holderBtn.Tele
	-----------------------------------------------------------------------------------------------------------[ END ]
	
	---------------------------------------------------------------------------------------------------[ Button Clone ]
	wait(1)
	for i,v in pairs(game.Players:GetPlayers()) do
		if v.Name == game.Players.LocalPlayer.Name then
			local clone = script.Parent.TextButton:Clone()
			clone.Parent = script.Parent.Parent.plrHolder.mainPlrHolder
			clone.Name = v.Name
			clone.Text = (' @')..v.DisplayName..(' (You)')
			clone.UIStroke.Color = Color3.fromRGB(234, 255, 0)
			clone.Visible = true
		else
			local clone = script.Parent.TextButton:Clone()
			clone.Parent = script.Parent.Parent.plrHolder.mainPlrHolder
			clone.Name = v.Name
			clone.Text = (' @')..v.DisplayName
			clone.Visible = true
		end
	end
	
	game.Players.PlayerRemoving:Connect(function(plr)
		for i,m in pairs(script.Parent.Parent.plrHolder.mainPlrHolder:GetChildren()) do
			if m:IsA('TextButton') and m.Name == plr.Name then
				m:Remove()
			end
		end
	end)
	
	game.Players.PlayerAdded:Connect(function(plrh)
		local clone = script.Parent.TextButton:Clone()
		clone.Parent = script.Parent.Parent.plrHolder.mainPlrHolder
		clone.Name = plrh.Name
		clone.Text = (' @')..plrh.DisplayName
		clone.Visible = true
	end)
	----------------------------------------------------------------------------------------------------[ END ]
	
	--------------------------------------------------------------------------------------------------------[ UI Drag ]
	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (20); -- // The speed of the UI drag.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
	--------------------------------------------------------------------------------------------------------------[ END ]
	
	-------------------------------------------------------------------------------------------------------------[ Button Functions ]
	script.Parent.Parent.infoFrame.holderBtn.Tele.MouseButton1Click:Connect(function()
		local find = game.Workspace:FindFirstChild(script.Parent.Parent.infoFrame.holderBtn.Tele.plr.Value):WaitForChild('HumanoidRootPart')
		
		if find then
			
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = find.CFrame
			
			game.StarterGui:SetCore("SendNotification", {
				Title = script.Parent.Parent.Frame.name.Text;
				Text = "Teleported To Player: "..script.Parent.Parent.infoFrame.holderBtn.Tele.plr.Value;
				Icon = "http://www.roblox.com/asset/?id=9830915901";
				Duration = 5;
			})
		end
	end)
	
	script.Parent.Parent.infoFrame.holderBtn.copyLink.MouseButton1Click:Connect(function()
		setclipboard("https://www.roblox.com/users/"..game.Players:FindFirstChild(script.Parent.Parent.infoFrame.holderBtn.Tele.plr.Value).UserId.."/profile")
		game.StarterGui:SetCore("SendNotification", {
			Title = script.Parent.Parent.Frame.name.Text;
			Text = "Copied Link To: "..script.Parent.Parent.infoFrame.holderBtn.Tele.plr.Value;
			Icon = "http://www.roblox.com/asset/?id=9830915901";
			Duration = 5;
		})
	end)
	
	script.Parent.Parent.infoFrame.holderBtn.espBtn.MouseButton1Click:Connect(function()
		
		pcall(function() loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua'))() end)
		
		game.StarterGui:SetCore("SendNotification", {
			Title = script.Parent.Parent.Frame.name.Text;
			Text = "ESP Executed";
			Icon = "http://www.roblox.com/asset/?id=9830915901";
			Duration = 5;
		})
	end)
	---------------------------------------------------------------------------------------------------------------[ END ]
	
	----------------------------------------------------------------------------------------------------------------[ Affect Giver]
	local Players = game:GetService('Players')
	
	-- Fetch the thumbnail
	local userId = 1
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	-- Set the ImageLabel's content to the user thumbnail
	local imageLabel = script.Parent.Parent.infoFrame.plrImage
	imageLabel.Image = content
	--------------------------------------------------------------------------------------------------------------[ END ]
	
	----------------------------------------------------------------------------------------------------[ Circle Click & Info ]
	repeat wait(1)
	for i,z in pairs(script.Parent.Parent.plrHolder.mainPlrHolder:GetChildren()) do
		if z:IsA('TextButton') then
			z.MouseButton1Click:Connect(function()
				script.Parent.Parent.infoFrame.userName.Text = ("Name: "..z.Name)
				script.Parent.Parent.infoFrame.userId.Text = ("ID: ")..game.Players:FindFirstChild(z.Name).UserId
				script.Parent.Parent.infoFrame.displayName.Text = ("Display: @")..game.Players:FindFirstChild(z.Name).DisplayName
				script.Parent.Parent.infoFrame.holderBtn.Tele.plr.Value = z.Name
				local Player = game.Players:FindFirstChild(z.Name)
	
				local joinTime = os.time() - (Player.AccountAge*86400)
				local joinDate = os.date("!*t", joinTime)
	
				script.Parent.Parent.infoFrame.accAge.Text = "Joined: "..joinDate.month.."/"..joinDate.day.."/"..joinDate.year
				local Players = game:GetService("Players")
	
				local player = z.Name
	
				-- Fetch the thumbnail
				local userId = Players:FindFirstChild(z.Name).UserId
				local thumbType = Enum.ThumbnailType.HeadShot
				local thumbSize = Enum.ThumbnailSize.Size420x420
				local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
				-- Set the ImageLabel's content to the user thumbnail
				local imageLabel = script.Parent.Parent.infoFrame.plrImage
				imageLabel.Image = content
	
				for i,g in ipairs(script.Parent.Parent.infoFrame.Frame.BorderHolder.mainPlrHolder:GetChildren()) do
					if g:IsA('ImageLabel') then
						g:Destroy()
					end
				end
	
				local info = game.Players:GetCharacterAppearanceInfoAsync(game.Players:FindFirstChild(z.Name).UserId)
				for i,v in ipairs(info['assets']) do
					if v then
						local clone = script.Parent.ImageLabel:Clone()
						clone.Parent = script.Parent.Parent.infoFrame.Frame.BorderHolder.mainPlrHolder
						clone.Image = 'rbxthumb://type=Asset&id='..v.id..'&w=420&h=420'
						clone.Visible = true
					end
				end
			end)
		end
	end
	until nil
	------------------------------------------------------------------------------------------------------[ END ]
end
coroutine.wrap(MISKEV_fake_script)()
local function ISLXQE_fake_script() -- handlerIntro.Main 
	local script = Instance.new('LocalScript', handlerIntro)

	wait(game:IsLoaded())
	---------------------------------------------------------------------------------------------------------------[ Stroke Giver ]
	--------------------------------------------------- mainFrame
	
	local sound = Instance.new('Sound')
	sound.Parent = script.Parent
	sound.Name = 'loaded'
	sound.SoundId = 'rbxassetid://6647898215'
	sound.PlaybackSpeed = 2
	
	local strokeNew = Instance.new('UIStroke')
	strokeNew.Parent = script.Parent
	local clone2 = script.Parent:WaitForChild('UIStroke'):Clone()
	clone2.Parent = script.Parent.Parent.Parent.BG
	clone2.Thickness = 2
	clone2.ApplyStrokeMode = 'Border'
	clone2.Color = Color3.fromRGB(255, 162, 0)
	clone2.Enabled = true
	
	wait(1)
	
	sound:Play()
	
	local Info = TweenInfo.new(0.3)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.Parent:FindFirstChild('BG'), Info, {Size = UDim2.new(0, 150,0, 150)})
	Tween:Play()
	
	wait(0.3)
	
	local Info = TweenInfo.new(0.3)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.Parent:FindFirstChild('loaderFrame'), Info, {Size = UDim2.new(0, 130,0, 130)})
	Tween:Play()
	
	script.Parent.Parent.TextLabel.Text = 'Welcome '..game.Players.LocalPlayer.Name
	
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	local imageLabel = script.Parent.Parent.ImageLabel
	imageLabel.Image = content
	
	wait(1)
	
	local Info = TweenInfo.new(0.5)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.ImageLabel, Info, {BackgroundTransparency = 0})
	Tween:Play()
	local Info = TweenInfo.new(0.5)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.ImageLabel, Info, {ImageTransparency = 0})
	Tween:Play()
	local Info = TweenInfo.new(0.5)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.TextLabel, Info, {TextTransparency = 0})
	Tween:Play()
	
	wait(3)
	
	local Info = TweenInfo.new(0.5)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.ImageLabel, Info, {BackgroundTransparency = 1})
	Tween:Play()
	local Info = TweenInfo.new(0.5)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.ImageLabel, Info, {ImageTransparency = 1})
	Tween:Play()
	local Info = TweenInfo.new(0.5)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.TextLabel, Info, {TextTransparency = 1})
	Tween:Play()
	
	wait(1)
	
	local Info = TweenInfo.new(0.3)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.Parent:FindFirstChild('loaderFrame'), Info, {Size = UDim2.new(0, 0,0, 0)})
	Tween:Play()
	
	wait(0.3)
	
	local Info = TweenInfo.new(0.3)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.Parent:FindFirstChild('BG'), Info, {Size = UDim2.new(0, 0,0, 0)})
	Tween:Play()
	
	wait(1)
	
	script.Parent.Parent.Parent:FindFirstChild('loaderFrame').Visible = false
	
	script.Parent.Parent.Parent:FindFirstChild('BG').Visible = false
	
	script.Parent.Parent.Parent.mainFrame:WaitForChild('UIStroke').Transparency = 1
	
	script.Parent.Parent.Parent.mainFrame.Visible = true
	
	wait(0.5)
	
	local Info = TweenInfo.new(0.3)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.Parent.mainFrame, Info, {Size = UDim2.new(0, 732,0, 453)})
	Tween:Play()
	
	wait(1)
	
	local Info = TweenInfo.new(0.2)
	local Tween = game:GetService('TweenService'):Create(script.Parent.Parent.Parent.mainFrame:WaitForChild('UIStroke'), Info, {Transparency = 0})
	Tween:Play()
	
	wait(1)
	
	game.StarterGui:SetCore("SendNotification", {
		Title = script.Parent.Parent.Parent.mainFrame.Frame.name.Text;
		Text = script.Parent.Parent.Parent.mainFrame.Frame.name.Text.." Has LOADED";
		Icon = "http://www.roblox.com/asset/?id=9830915901";
		Duration = 5;
	})
	
	script.Parent.Parent.Parent.BG:Destroy()
	script.Parent.Parent.Parent.loaderFrame:Destroy()
end
coroutine.wrap(ISLXQE_fake_script)()
